<div class="devise-inner">
<h2>ログイン</h2>
<br>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.label :email, "Eメール" %><br />
    <%= f.email_field :email, id: "email", autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.label :password, "パスワード" %><br />
    <%= f.password_field :password, autocomplete: "current-password" %>
  </div>

  <% if devise_mapping.rememberable? %>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me, "記憶する" %>
    </div>
  <% end %>
<br>
  <div class="actions">
    <%= f.submit "ログイン",class: "button" %>
  </div>
<% end %>


</div>